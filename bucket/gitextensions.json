    "homepage": "https://gitextensions.github.io/",
    "license": "GPL-3.0-only",
    "architecture": {
        "64bit": {
            "url": "https://github.com/gitextensions/gitextensions/releases/download/v6.0.3/GitExtensions-Portable-x64-6.0.3.18335-6b4f4bf6e.zip",
